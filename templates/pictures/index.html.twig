{% extends 'base.html.twig' %}

{% block title %}Hello PicturesController!{% endblock %}

{% block body %}

    <div class="row row-cols-1 row-cols-md-2">
        {% for image in images %}
            <ul>
                <li>{{ image.category.name }}</li>
            </ul>
            <img src="{{ image.source }}">

        {% endfor %}
    </div>
{% endblock %}
